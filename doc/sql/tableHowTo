use for configuration http://localhost/phpmyadmin/
# apt-get install apache2 phpmyadmin libapache2-mod-php5 php5-mysql

enable InnoDB.
# vi /etc/mysql/my.cnf
#skip-innodb
innodb_data_home_dir=/var/lib/mysql
innodb_data_file_path=ibdata1:10M:autoextend

# /etc/init.d/mysql restart

create a new user "flyhigh" with password "flyhigh" from localhost with 
these rights:
SELECT, INSERT, UPDATE, DELETE, CREATE, DROP, FILE, INDEX, ALTER, CREATE 
TEMPORARY TABLES

create initial database
$ mysql -u root -p < flyhigh.sql
Enter password:


OTHERS
backup the database
$ mysqldump flyhigh -h localhost -u flyhigh -p > flyhigh.backup

restore the database
$ mysql -u flyhigh -p < flyhigh.backup
Enter password:

remove the database
$ mysqladmin -u flyhigh -p drop flyhigh
Enter password:

